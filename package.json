{
  "name": "bugger",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run setup:client && npm run setup:server && npm run build:client",
    "setup:client": "cd bugger.client && npm i",
    "setup:server": "cd bugger.server && npm i",
    "build:client": "npm run --prefix bugger.client build",
    "start": "node bugger.server/index.js"
  },
  "engines": {
    "node": "^14.15.4"
  },
  "dependencies": {
    "axios": "^0.21.1"
  }
}
